{
  "name": "pico-check",
  "version": "0.0.1",
  "description": "An incredibly tiny javascript testing library",
  "main": "pico-check.js",
  "scripts": {
    "lint": "eslint --fix **/*.js",
    "dev": "nodemon --exec npm run test",
    "test": "cd ./tests && npm run test",
    "test:cli": "cd ./tests/cli-test && npm run test",
    "test:simple": "nodemon tests/simple.test.js",
    "test:error": "nodemon tests/error.test.js",
    "test:lib": "nodemon tests/lib.test.js",
    "test:builder": "nodemon tests/builder.test.js",
    "test:utils": "nodemon tests/utils.test.js",
    "utils": "cd ./tests && npm run utils",
    "verbose": "cd ./tests && npm run verbose",
    "mini": "cd ./tests && npm run mini"
  },
  "bin": "src/cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stolksdorf/pico-check.git"
  },
  "keywords": [
    "test",
    "assert"
  ],
  "eslintIgnore": [
    "node_modules"
  ],
  "author": "stolksdorf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stolksdorf/pico-check/issues"
  },
  "homepage": "https://github.com/stolksdorf/pico-check#readme",
  "dependencies": {
    "ansi-styles": "^3.2.0",
    "chalk": "^2.3.0",
    "chokidar": "^2.0.0",
    "commander": "^2.12.2",
    "concordance": "^3.0.0",
    "glob": "^7.1.2"
  }
}
